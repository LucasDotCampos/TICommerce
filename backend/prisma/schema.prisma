// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Worker {
  id           String      @id @default(uuid())
  createdAt    DateTime    @default(now())
  cpf          String      @unique
  password     String
}

model Product {
  id String @id @default(uuid())
  createdAt DateTime @default(now())
  name String  
  price Int
  quantity Int
  image String
}

